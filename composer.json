{
    "name": "php-solution/doctrine-orm-extra-lib",
    "type": "library",
    "description": "This library contains additional functionality for work with doctrine orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksandr Savchenko",
            "email": "olsav@ciklum.com"
        },
        {
            "name": "Nikita Kalinin",
            "email": "kalinick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/orm": "~2.5",
        "php-solution/std-lib": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0"
    },
    "suggest": {
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\Doctrine\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}